@using Shop.Pages.Models
@using Shop.ViewModels
@model CarsListViewModels
@{
    Layout = "_Layout";
}

<!DOCTYPE html>


    <h1>Все автомобили</h1>
    <h2>@Model.currCategory</h2>
    <div class="row mt-5 mb-2">
    @{
        foreach(Car car in Model.AllCars)
        {
            @Html.Partial("AllCars",car)
        }
    }
    </div>